{
  "name": "@vi-space/common-ui",
  "type": "module",
  "version": "0.0.1",
  "description": "vi-space common ui component.",
  "author": "Seventhse <season.sevent@icloud.com> (https://github.com/seventhse)",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/index.css",
    "./*": "./dist/**/*.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build:styles": "tailwindcss -i src/styles.css -o ./dist/index.css --minify",
    "build:components": "tsdown",
    "dev:styles": "tailwindcss -i src/styles.css -o ./dist/index.css --watch",
    "dev:components": "tsdown --watch",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.0",
    "@radix-ui/react-alert-dialog": "1.1.14",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-collapsible": "1.1.11",
    "@radix-ui/react-context-menu": "2.2.15",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-hover-card": "1.1.14",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-menubar": "1.1.15",
    "@radix-ui/react-navigation-menu": "1.2.13",
    "@radix-ui/react-popover": "1.1.14",
    "@radix-ui/react-radio-group": "1.3.7",
    "@radix-ui/react-scroll-area": "1.2.9",
    "@radix-ui/react-select": "2.2.5",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.5",
    "@radix-ui/react-tabs": "1.1.12",
    "@radix-ui/react-toggle": "1.1.9",
    "@radix-ui/react-toggle-group": "1.1.10",
    "@radix-ui/react-tooltip": "1.2.7",
    "@vi-space/utils": "workspace:*",
    "class-variance-authority": "0.7.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "embla-carousel-react": "8.6.0",
    "lucide-react": "0.525.0",
    "react-day-picker": "9.8.0",
    "react-hook-form": "7.60.0",
    "react-resizable-panels": "3.0.3",
    "sonner": "2.0.6",
    "tw-animate-css": "1.3.5",
    "vaul": "1.1.2",
    "zod": "4.0.5"
  },
  "devDependencies": {
    "@storybook/addon-docs": "9.0.17",
    "@storybook/react-vite": "9.0.17",
    "@tailwindcss/cli": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vi-space/eslint-config": "workspace:*",
    "@vi-space/internal": "workspace:*",
    "@vi-space/typescript-config": "workspace:*",
    "eslint": "catalog:",
    "eslint-plugin-storybook": "^9.0.17",
    "storybook": "^9.0.17",
    "tailwindcss": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  }
}
