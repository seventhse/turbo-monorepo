{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": [
      "ES2022"
    ],
    "types": [
      "node"
    ],
    "outDir": "dist",
