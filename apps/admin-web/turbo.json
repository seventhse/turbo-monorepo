{
  "$schema": "https://turborepo.com/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "@vi-space/utils#build",
        "@vi-space/common-ui#build"
      ]
    },
    "build:styles": {
      "outputs": [
        "dist/**"
      ]
    },
    "build:components": {
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "with": [
        "@vi-space/utils#build",
        "@vi-space/common-ui#build"
      ]
    }
  }
}
